{% if success is not null %}
    {% if success %}
        <div class="ft-success" style="color: green;">Import successful for entered ICO!</div>
    {% else %}
        <div class="ft-fail" style="color: red;">Import failed. Please check the entered Company ICO.</div>
    {% endif %}
{% endif %}

{{ form_start(form) }}
    {{ form_row(form.companyId, {'label': 'Company ICO'}) }}
